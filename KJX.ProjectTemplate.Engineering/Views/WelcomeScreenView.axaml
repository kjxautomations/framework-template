<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:KJX.ProjectTemplate.Engineering.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="KJX.ProjectTemplate.Engineering.Views.WelcomeScreenView"
             x:DataType="viewModels:WelcomeScreenViewModel">
    <Label Content="{Binding WelcomeText}"
           FontSize="24" FontWeight="Bold"
           VerticalAlignment="Center" HorizontalAlignment="Center"
           VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
</UserControl>
